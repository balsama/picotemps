{
    "name": "balsama/picotemps",
    "description": "Grabs temps from a network of Raspbery Pi Pico Ws and sends them to google sheets",
    "type": "project",
    "require": {
        "guzzlehttp/guzzle": "^7",
        "revolution/laravel-google-sheets": "^6.0",
        "catfan/medoo": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "license": "gpl-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Balsama\\Tempbot\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "./vendor/bin/phpcs --standard=PSR12 ./src ./tests",
        "test": "./vendor/bin/phpunit tests"
    }
}
